{
  "name": "what-to-watch",
  "version": "1.0.0",
  "description": "Full Stack movie search app",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "deploy": "npm build && git add . && git commit -m \"Building for production\" && git push heroku master",
    "test": "npm run test --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipsum13/What-To-Watch.git"
  },
  "author": "Sam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ipsum13/What-To-Watch/issues"
  },
  "homepage": "https://github.com/ipsum13/What-To-Watch#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.3",
    "express-validator": "^6.5.0",
    "google-auth-library": "^6.0.1",
    "gravatar": "^1.8.0",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "normalize-url": "^5.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
